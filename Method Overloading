package BankAccount
     static long add(long a,long b){
            return a+b;
    }
    static int add(int a,int b,int c){
            return a+b+c;
       }
public class Main {
    public static void main(String[] args){
       System.out.println(BankAccount.add(679334857,34563456));
       System.out.println(BankAccount.add(456,456,456);
    }
        
    }
    
}
